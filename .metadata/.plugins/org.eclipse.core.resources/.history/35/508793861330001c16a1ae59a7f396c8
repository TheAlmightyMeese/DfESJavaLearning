package com.qa.main;

public class Exer2 {
	public static void main(String[] args) {
	
		boolean myBool = false;
		byte myByte = 3;
		char myChar = 'r';
		short myShort = 0; 
		int myInt = 0;
		long myLong = 1l;
		float myFloat = 5.0f;
		double myDouble = 0.000000d;
		
		System.out.println( "H" + (myByte) + "LL" + myInt +
				" W" + myDouble + myChar + myLong + "d " + myFloat + " " + myBool);

	//	H3LL0 W0.000000r1d 5.0 false
}
}