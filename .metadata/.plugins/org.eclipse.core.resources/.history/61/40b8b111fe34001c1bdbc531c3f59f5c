package com.qa.main;

import com.qa.garage.*;

public class Runner {
public static void main(String[] args) {
	
	Garage storage = new Garage();
	
	Vehicle RT30GRT = new Car("BMW", 2000, true, "Black", 3, 160);
	Vehicle LB60KUV = new Car("Peugeot", 2000, false, "SIlver", 5, 120);

	storage.addVehicle(LB60KUV);
	storage.addVehicle(RT30GRT);
	storage.checkGarage();
	storage.searchGarage(0).fixCost();
	RT30GRT.fixCost();
	
storage.editVehicle(1, LB60KUV);
storage.checkGarage();
	

System.out.println(LB60KUV.topSpeed);
}
}
