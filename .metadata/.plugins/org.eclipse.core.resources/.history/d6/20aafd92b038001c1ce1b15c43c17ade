package com.qa.enumlist;

import java.util.Random;

import com.qa.enumlist.Card.Ranks;
import com.qa.enumlist.Card.Suits;

public class Dealer {

	public Suits Pullsuit() {
		return Suits.values()[new Random().nextInt(Suits.values().length)];
		}
	public static String Pullrank() {
		String Rank = Ranks.values()[new Random().nextInt(Ranks.values().length)].toString();
		return Rank;
	}
	public String Draw;
	
		
	
}
