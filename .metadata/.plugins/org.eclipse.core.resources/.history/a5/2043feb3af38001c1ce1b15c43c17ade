package com.qa.enumlist;

import java.util.Random;

import com.qa.enumlist.Card.Ranks;
import com.qa.enumlist.Card.Suits;

public class Dealer {

	public static String PullCard() {
		int suit =  new Random().nextInt(Suits.values().length);
		int rank = new Random().nextInt(Ranks.values().length);
		String draw = rank + "of " + suit;
		return draw;
		
	}
	
	
	
}
