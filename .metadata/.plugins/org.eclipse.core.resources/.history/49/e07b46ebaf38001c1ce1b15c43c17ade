package com.qa.enumlist;

import java.util.Random;

import com.qa.enumlist.Card.Ranks;
import com.qa.enumlist.Card.Suits;

public class Dealer {

	public Suits Pullsuit() {
		return Suits.values()[new Random().nextInt(Suits.values().length)];
		}
	public Ranks Pullrank() {
		return Ranks.values()[new Random().nextInt(Ranks.values().length)];
	}
	public String Draw;
		
	
}
