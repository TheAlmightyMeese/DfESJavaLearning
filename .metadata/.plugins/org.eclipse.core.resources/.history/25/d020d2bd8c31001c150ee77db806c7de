package com.qa.day3;

public class CompIf {

		public static int blackJack(int card1, int card2) {
			if (card1 > card2 && card1 <= 21) {
				return card1;
				}
				else if (card1 > 21) {
					return 0;
				}
				else if (card2 > card1 && card2 <= 21) {
					return card2;					
				}
				else { return 0;}
			
			
		}
		}

